<script type="text/javascript">
var jdtongdian= $('#jdtongdian');
function doAjaxJingdongMallProduct(id, cid3){
  $.ajax({
    url: "https://api.uuhaodian.com/jduu/search_by_shop",
    type: 'GET',
    dataType: 'jsonp',
    data:{
      id: id,
      cid3: cid3,
      callback: 'mallCallback'
    },
    success:function(data){
    }
  });
}
var mallCallback = function(data){
  if(data.status == 200){
    Util.createJdList(data.results,jdtongdian,channel,'同店推荐');
  }
};
$(function(){
  doAjaxJingdongMallProduct(<%= seller_id %>, <%= cid3 %>);
});
</script>
