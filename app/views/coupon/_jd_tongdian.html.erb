<script type="text/javascript">
var jdtongdian= $('#jdtongdian');
function doAjaxJingdongMallProduct(id){
  $.ajax({
    url: "https://api.uuhaodian.com/ddk/jd_mall_products",
    type: 'GET',
    dataType: 'jsonp',
    data:{
      id: id,
      callback: 'mallCallback'
    },
    success:function(data){
    }
  });
}
var mallCallback = function(data){
  if(data.status_code == 200){
    Util.createJdList(data.data.data,jdtongdian,channel,'同店推荐');
  }
};
$(function(){
  doAjaxJingdongMallProduct(<%= seller_id %>);
});
</script>
